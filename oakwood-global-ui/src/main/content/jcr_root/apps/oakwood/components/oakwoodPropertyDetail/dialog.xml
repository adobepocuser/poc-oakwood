<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    height="400"
    stateful="false"
    xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">
				<propertyDetails
                    jcr:primaryType="cq:Panel"
                    title="Property Details">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <Property_x0020_Name
                            jcr:primaryType="cq:Widget"
                            fieldDescription="Provide the property Name."
                            fieldLabel="Property Name"
                            name="./propertyName"
                            xtype="textfield"/>
                        <propertyAddress
                            jcr:primaryType="cq:Widget"
                            fieldDescription="Provide the property address."
                            fieldLabel="Property Address"
                            name="./propertyAddress"
                            xtype="textarea"/>
                        <conatctNumber
                            jcr:primaryType="cq:Widget"
                            fieldDescription="Provide the contact number."
                            fieldLabel="Contact Number"
                            name="./propertyNumber"
                            xtype="textfield"/>
                    </items>
                </propertyDetails>
                <basic
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_basic.infinity.json"
                    xtype="cqinclude"/>
                <advanced
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_advanced.infinity.json"
                    xtype="cqinclude"/>
                <image
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_image.infinity.json"
                    xtype="cqinclude"/>
                <cloudservices
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_cloudservices.infinity.json"
                    xtype="cqinclude"/>
                <blueprint
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_blueprint.infinity.json"
                    xtype="cqinclude"/>
                <livecopy
                    jcr:primaryType="cq:Widget"
                    path="/libs/foundation/components/page/tab_livecopy.infinity.json"
                    xtype="cqinclude"/>
            </items>
        </tabs>
    </items>
</jcr:root>
